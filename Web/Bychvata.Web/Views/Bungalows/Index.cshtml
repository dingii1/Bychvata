@model IEnumerable<Bychvata.Web.ViewModels.Models.ViewModels.BungalowViewModel>

@{
    ViewData["Title"] = "Бунгала";
}

<h1>@this.ViewData["Title"]</h1>

<table class="table table-striped text-center">
    <thead class="bg-info">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rooms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Beds)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Notes)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rooms)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Beds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notes)
            </td>
            <td>
                @Html.ActionLink("Детайли", "Details", new { id = item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
<div class="row">
    <div class="col-12">
        <img src="~/20180715_055732.jpg" style="width:100%" height="600" />
    </div>
</div>
